pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building step..123'
                sleep 30
            }
        }
        stage('Test') {
            steps {
                echo 'Testing step..1'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying step....1'
            }
        }
    }
}
